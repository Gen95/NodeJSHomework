{% set block = "table" %}


<div class="{{ block }}{% if mods %} {{block}}__{{mods}}{% endif %}">
{% if content %}
       {{content}}
{% else %}
<div class="{{ block }}__row {{ block }}__head">
  <div class="{{ block }}__cell {{ block }}__cell_name">Name</div>
  <div class="{{ block }}__cell {{ block }}__cell_last-commit">Last commit</div>
  <div class="{{ block }}__cell {{ block }}__cell_commit-message">Commit message</div>
  <div class="{{ block }}__cell {{ block }}__cell_committer">Committer</div>
  <div class="{{ block }}__cell {{ block }}__cell_updated">Updated</div>
</div>
<div class="table__body js-folder-list">
  {# <div class="{{ block }}__row">
