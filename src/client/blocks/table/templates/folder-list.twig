      <a class="link" href="#">{{row.lastCommit}}</a>
    </div>
    <div class="{{ block }}__cell {{ block }}__cell_commit-message {{ block }}__cell_md-size_1 {{ block }}__cell_md-order_2">{{row.commitMessage}}</div>
    <div class="{{ block }}__cell {{ block }}__cell_committer committer {{ block }}__cell_md-size_auto {{ block }}__cell_md-order_4">{{row.committer}}</div>
    <div class="{{ block }}__cell {{ block }}__cell_updated {{ block }}__cell_md-size_auto {{ block }}__cell_md-order_5">{{row.updated}}</div>
  </div> #}
</div>
{% endif %} 
</div>